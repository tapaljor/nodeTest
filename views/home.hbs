{{>header}}
<form action="/submit" method="POST">
     <label for="name">Unit:</label>
        <input type="number" id="unit" name="unit" min="1000" max="90000" placeholder="Unit# (1000-90000)"><br><br>
     <label for="name">Name:</label>
        <input type="text" id="name" name="name" value="Sarah Johnson"><br><br>
        <label for="age">Age:</label>
        <input type="number" id="age" name="age" value="30" ><br><br>
        <label for="gender">Gender:</label>
        <input type="text" id="gender" name="gender" value="female" ><br><br>
        <fieldset>
            <legend>Address</legend>
            <label for="street">Street:</label>
            <input type="text" id="street" name="address[street]" value="303 Elm Street" ><br><br>
            <label for="city">City:</label>
            <input type="text" id="city" name="address[city]" value="Springfield" ><br><br>
            <label for="state">State:</label>
            <input type="text" id="state" name="address[state]" value="IL" ><br><br>
            <label for="zip">ZIP Code:</label>
            <input type="text" id="zip" name="address[zip]" value="62704" ><br><br>
        </fieldset>
        <label for="phoneNumber">Phone Number:</label>
        <input type="tel" id="phoneNumber" name="phoneNumber" value="555-1235" ><br><br>
        <label for="email">Email:</label>
        <input type="email" id="email" name="email" value="sarahjohnson@example.com" ><br><br>
        <input type="submit" value="Submit">
</form>
{{>footer}}
